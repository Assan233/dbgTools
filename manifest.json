{
    "name": "dbgTool",
    "version": "0.1",
    "description": "",
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "icons": {
        "128": "./assets/icon.png"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "scripting",
        "http://*/*",
        "https://*/*"
    ],
    "action": {},
    "web_accessible_resources": [
        {
            "resources": [
                "sdk.js"
            ],
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "extension_ids": [
                "mkplamemkjkbiokaobmjbnpmcahkonjo"
            ],
            "use_dynamic_url": true
