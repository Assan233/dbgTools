    ],
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "./content/index.js"
            ],
            "css": [
                "./content/index.css"
            ],
            "all_frames": true
        }
    ],
    "manifest_version": 3
}